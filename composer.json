{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "fresns/fresns",
    "type": "project",
    "description": "Cross-platform general-purpose multiple content forms social network service software.",
    "keywords": [
        "fresns",
        "laravel",
        "sns",
        "social",
        "club",
        "group",
        "bbs",
        "forum",
        "community",
        "communities"
    ],
    "license": "Apache-2.0",
    "homepage": "https://fresns.org",
    "support": {
        "email": "support@fresns.org",
        "forum": "https://discuss.fresns.org",
        "wiki": "https://fresns.org",
        "docs": "https://docs.fresns.com/open-source/",
        "issues": "https://github.com/fresns/fresns/issues",
        "source": "https://github.com/fresns/fresns"
    },
    "authors": [
        {
            "name": "Jevan Tang",
            "email": "jevan@fresns.org",
            "homepage": "https://github.com/jevantang",
            "role": "Creator"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.38",
        "laravel/tinker": "^2.10",
        "laravel/ui": "^4.6",
        "fresns/dto": "^1.3",
        "fresns/cmd-word-manager": "^1.5",
        "fresns/plugin-manager": "^3.2",
        "fresns/theme-manager": "^3.0",
        "fresns/market-manager": "^4.0",
        "hisorange/browser-detect": "^5.0",
        "nelexa/zip": "^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "laravel/pail": "^1.2",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.40",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "phpunit/phpunit": "^11.5",
        "spatie/laravel-error-share": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\""
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "plugins/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": true,
            "merge-extra-deep": true
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": false,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
